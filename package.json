{
  "name": "jbrlloyd-blog-astro",
  "type": "module",
  "version": "0.3.1",
  "packageManager": "pnpm@8.6.12",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "HIDE_DRAFTS=true astro check && astro build",
    "preview:astro": "pnpm run build && astro preview",
    "preview": "HIDE_DRAFTS=true pnpm run build && wrangler pages dev --node-compat --live-reload ./dist",
    "deploy": "pnpm run build && wrangler pages deploy dist --project-name blog-jbrlloyd",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/check": "^0.8.3",
    "@astrojs/cloudflare": "^11.0.1",
    "@astrojs/mdx": "^3.1.3",
    "@astrojs/rss": "^4.0.7",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/solid-js": "^4.4.0",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/ts-plugin": "^1.9.0",
    "@expressive-code/plugin-line-numbers": "^0.35.3",
    "@nanostores/solid": "^0.4.2",
    "@playform/compress": "^0.1.0",
    "@types/uuid": "^10.0.0",
    "astro": "^4.12.2",
    "astro-expressive-code": "^0.35.3",
    "fuse.js": "^7.0.0",
    "nanostores": "^0.10.3",
    "rehype-accessible-emojis": "^0.3.2",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-toc": "^9.0.0",
    "sharp": "^0.33.4",
    "solid-devtools": "^0.30.1",
    "solid-js": "^1.8.17",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.13",
    "wrangler": "^3.60.3"
  }
}